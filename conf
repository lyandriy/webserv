server
{
    listen 8080;
    server_name localhost;
    root html/test;
    accept_method GET;
    index index.html;
    error_page 404 /hola/hola1;
    location /hola
    {
        index hola2;
        error_page 404 error/400.html;
    }
}

server
{
    listen 8081;
    server_name localhost;
    root html/test;
    accept_method GET;
}
